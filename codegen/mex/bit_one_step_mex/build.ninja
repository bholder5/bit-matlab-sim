# CompilerName=gcc
# Mode=debug
ninja_required_version = 1.3

# Basic folders
root = .
builddir = $root/build/glnxa64
matlabdir = /usr/local/MATLAB/R2021b
startdir = /home/brad/bit-matlab-sim

# Toolchain information
cc = /usr/bin/gcc
cxx = g++
linker = /usr/bin/gcc
linkerxx = g++

cflags =  -fexceptions -fPIC -fno-omit-frame-pointer -pthread -D_GNU_SOURCE -DMATLAB_MEX_FILE  -g -std=c99 -I "$root" -I "$startdir" -I "$startdir/ADCS" -I "$startdir/Miscellaneous" -I "$startdir/Plant_functions" -I "$root/interface" -I "$matlabdir/extern/include" -I "." -DHAVE_LAPACK_CONFIG_H -DLAPACK_COMPLEX_STRUCTURE -DMW_HAVE_LAPACK_DECLS -DMODEL=bit_one_step_mex -c 

cxxflags =  -fexceptions -fPIC -fno-omit-frame-pointer -pthread -std=c++11 -D_GNU_SOURCE -DMATLAB_MEX_FILE  -g -std=c++11 -I "$root" -I "$startdir" -I "$startdir/ADCS" -I "$startdir/Miscellaneous" -I "$startdir/Plant_functions" -I "$root/interface" -I "$matlabdir/extern/include" -I "." -DHAVE_LAPACK_CONFIG_H -DLAPACK_COMPLEX_STRUCTURE -DMW_HAVE_LAPACK_DECLS -DMODEL=bit_one_step_mex -c 

ldflags =  -pthread -Wl,--no-undefined -Wl,-rpath-link,$matlabdir/bin/glnxa64 -shared  -L"$matlabdir/bin/glnxa64" -lmx -lmex -lmat -lm -lstdc++  -g -o bit_one_step_mex.mexa64 -L"$matlabdir/bin/glnxa64" -lmwlapack -lmwblas -lemlrt -lcovrt -lut -lmwmathutil  -Wl,--version-script,bit_one_step_mex.map

# Build Rules
rule cc
    command = $cc $cflags $in -o $out

rule cxx
    command = $cxx $cxxflags $in -o $out

rule link
    command = $linker $in $ldflags

rule linkxx
    command = $linkerxx $in $ldflags

# Build
build $builddir/bit_one_step_mex_data.o : cc $root/bit_one_step_mex_data.c
build $builddir/rt_nonfinite.o : cc $root/rt_nonfinite.c
build $builddir/bit_one_step_mex_initialize.o : cc $root/bit_one_step_mex_initialize.c
build $builddir/bit_one_step_mex_terminate.o : cc $root/bit_one_step_mex_terminate.c
build $builddir/bit_one_step.o : cc $root/bit_one_step.c
build $builddir/axis2rot.o : cc $root/axis2rot.c
build $builddir/mass_mat_func.o : cc $root/mass_mat_func.c
build $builddir/compute_angular_velocity_C.o : cc $root/compute_angular_velocity_C.c
build $builddir/compute_rotation_mat_C.o : cc $root/compute_rotation_mat_C.c
build $builddir/rot2axis_C.o : cc $root/rot2axis_C.c
build $builddir/_coder_bit_one_step_mex_api.o : cc $root/interface/_coder_bit_one_step_mex_api.c
build $builddir/_coder_bit_one_step_mex_mex.o : cc $root/interface/_coder_bit_one_step_mex_mex.c
build $builddir/chol.o : cc $root/chol.c
build $builddir/mldivide.o : cc $root/mldivide.c
build $builddir/eml_int_forloop_overflow_check.o : cc $root/eml_int_forloop_overflow_check.c
build $builddir/_coder_bit_one_step_mex_info.o : cc $root/interface/_coder_bit_one_step_mex_info.c
build $builddir/c_mexapi_version.o : cc $matlabdir/extern/version/c_mexapi_version.c

# Link
build $root/bit_one_step_mex.mexa64 : link $builddir/bit_one_step_mex_data.o $builddir/rt_nonfinite.o $builddir/bit_one_step_mex_initialize.o $builddir/bit_one_step_mex_terminate.o $builddir/bit_one_step.o $builddir/axis2rot.o $builddir/mass_mat_func.o $builddir/compute_angular_velocity_C.o $builddir/compute_rotation_mat_C.o $builddir/rot2axis_C.o $builddir/_coder_bit_one_step_mex_api.o $builddir/_coder_bit_one_step_mex_mex.o $builddir/chol.o $builddir/mldivide.o $builddir/eml_int_forloop_overflow_check.o $builddir/_coder_bit_one_step_mex_info.o $builddir/c_mexapi_version.o 
